{
  "name": "copy-that-front",
  "version": "0.0.0",
  "description": "The frontend for copythat.io",
  "main": "index.js",
  "repository": "https://github.com/mulholio/copy-that-front",
  "author": "James Mulholland <james@copythat.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "eslint": "./node_modules/.bin/eslint ./pages ./lib ./components",
    "lint": "node_modules/.bin/prettier-eslint --write --print-width 70 --tab-width 2 --trailing-comma es5 --bracket-spacing --semi --single-quote \"{pages, components, lib}/**/*.{js,jsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "node_modules/.bin/prettier-eslint --write --print-width 70 --tab-width 2 --trailing-comma es5 --bracket-spacing --semi --single-quote",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-boost": "^0.1.22",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "next": "^7.0.2",
    "next-with-apollo": "^3.3.0",
    "react": "^16.6.3",
    "react-apollo": "^2.3.1",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@evermind/eslint-config-base": "^1.11.1",
    "babel-plugin-styled-components": "^1.9.1",
    "eslint": "^5.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.11.1"
  }
}
