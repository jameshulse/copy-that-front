{
  "name": "copy-that-front",
  "version": "0.0.0",
  "description": "The frontend for copythat.io",
  "main": "index.js",
  "repository": "https://github.com/mulholio/copy-that-front",
  "author": "James Mulholland <james@copythat.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "tsc && next",
    "build": "tsc && next build",
    "start": "tsc && next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "node_modules/.bin/prettier-eslint --write --print-width 70 --tab-width 2 --trailing-comma es5 --bracket-spacing --semi --single-quote",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-boost": "^0.1.22",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "next": "^7.0.2",
    "next-with-apollo": "^3.3.0",
    "react": "^16.6.3",
    "react-apollo": "^2.3.1",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@types/next": "^7.0.5",
    "@types/styled-components": "^4.1.3",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-styled-components": "^1.9.1",
    "husky": "^1.2.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  }
}
